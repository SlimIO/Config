{
  "name": "@slimio/config",
  "version": "0.13.0",
  "description": "SlimIO Reactive JSON Config loaded",
  "main": "index.js",
  "scripts": {
    "prepublishOnly": "pkg-ok",
    "test": "ava --verbose",
    "doc": "jsdoc -c ./jsdoc.json -r",
    "coverage": "nyc ava",
    "version": "conventional-changelog -i CHANGELOG.md -s",
    "report": "nyc report --reporter=html",
    "typedoc": "typedoc --out docs --name 'Config' --mode file --includeDeclarations --excludeExternals --hideGenerator --excludePrivate index.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SlimIO/Config.git"
  },
  "engines": {
    "npm": ">=6.0.0",
    "node": ">=10.1.0"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "lines": 98,
    "statements": 98,
    "functions": 98,
    "branches": 90
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "SlimIO",
    "Config",
    "JSON",
    "Schema",
    "Safe",
    "Loader",
    "observable",
    "reactive",
    "hotreload"
  ],
  "author": "SlimIO",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SlimIO/Config/issues"
  },
  "homepage": "https://github.com/SlimIO/Config#readme",
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@escommunity/minami": "^1.0.0",
    "@slimio/eslint-config": "^2.0.3",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.set": "^4.3.6",
    "@types/node": "^10.12.20",
    "@types/zen-observable": "^0.8.0",
    "ava": "^1.2.0",
    "conventional-changelog-cli": "^2.0.11",
    "eslint": "^5.12.1",
    "husky": "^1.3.1",
    "nyc": "^13.1.0",
    "pkg-ok": "^2.3.1",
    "typedoc": "^0.14.2"
  },
  "dependencies": {
    "@slimio/is": "^1.5.0",
    "ajv": "^6.7.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "node-watch": "^0.6.0",
    "zen-observable": "^0.8.13"
  }
}
