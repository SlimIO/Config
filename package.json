{
  "name": "@slimio/config",
  "version": "0.16.0",
  "description": "SlimIO Reactive JSON Config loaded",
  "main": "index.js",
  "scripts": {
    "prepublishOnly": "pkg-ok",
    "test": "cross-env psp && nyc --reporter=lcov ava --verbose",
    "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
    "coverage": "codecov",
    "version": "conventional-changelog -i CHANGELOG.md -s",
    "report": "nyc report --reporter=html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SlimIO/Config.git"
  },
  "engines": {
    "npm": ">=6.0.0",
    "node": ">=12"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "lines": 50,
    "statements": 50,
    "functions": 50,
    "branches": 50
  },
  "husky": {
    "hooks": {
      "pre-push": "cross-env eslint index.js src/**.js && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "SlimIO",
    "Config",
    "JSON",
    "Schema",
    "Safe",
    "Loader",
    "observable",
    "reactive",
    "hotreload"
  ],
  "files": [
    "index.js",
    "index.d.ts",
    "src"
  ],
  "author": "SlimIO",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SlimIO/Config/issues"
  },
  "homepage": "https://github.com/SlimIO/Config#readme",
  "devDependencies": {
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@escommunity/minami": "^1.0.0",
    "@slimio/eslint-config": "^4.1.0",
    "@slimio/psp": "^0.11.1",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.set": "^4.3.6",
    "@types/zen-observable": "^0.8.0",
    "ava": "^3.10.1",
    "codecov": "^3.7.0",
    "conventional-changelog-cli": "^2.0.34",
    "cross-env": "^7.0.2",
    "eslint": "^7.4.0",
    "husky": "^4.2.5",
    "jsdoc": "^3.6.4",
    "nyc": "^15.1.0",
    "pkg-ok": "^2.3.1"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "@slimio/is": "^1.5.1",
    "ajv": "^6.12.2",
    "lodash.clonedeep": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "node-watch": "^0.6.4",
    "zen-observable": "^0.8.15"
  }
}
